{
  "plugins": [
    "@semantic-release/commit-analyzer",
    {
      "preset": "eslint",
      "releaseRules": [
        {
          "tag": "Docs",
          "release": "patch"
        },
        {
          "tag": "Refactor",
          "release": "patch"
        },
        {
          "tag": "Build",
          "release": "patch"
        },
        {
          "tag": "Style",
          "release": "patch"
        },
        {
          "tag": "Fix",
          "release": "minor"
        }
      ],
      "parserOpts": {
        "noteKeywords": [
          "BREAKING CHANGE",
          "BREAKING CHANGES"
        ]
      }
    },
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/github"
  ],
  "branches": [
    "main",
    "release"
  ]
}
